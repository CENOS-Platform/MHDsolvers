#!/bin/sh
cd ${0%/*} || exit 1    # run from this directory
set -x

wmake simpleFoamEpot
wmake pimpleFoamEpot
wmake pimpleFoamEpotTransient
wmake buoyantFoamEpot
wmake buoyantFoamEpotTransient
wmake chtMultiRegionFoamEpot
wmake chtMultiRegionFoamEpotTransient
wmake initializeTemperature
wmake initializeMultiRegionTemperature

